---
import Layout from '../layouts/Layout.astro';
import StickyHeader from '../components/StickyHeader.astro';
import Hero from '../components/Hero.astro';
import SocialLinks from '../components/SocialLinks.astro';
import ValuePropsSection from '../components/ValuePropsSection.astro';
import ServicesSection from '../components/ServicesSection.astro';
import MusicSection from '../components/MusicSection.astro';
import VideoGrid from '../components/VideoGrid.astro';
import InstagramFeed from '../components/InstagramFeed.astro';
import ContactSection from '../components/ContactSection.astro';
---

<Layout title="DJ SNABU - Professional DJ Services">
  <StickyHeader />
  <Hero />
  <SocialLinks />
  <ValuePropsSection />
  <ServicesSection />
  <MusicSection />
  <VideoGrid />
  <InstagramFeed 
    instagramUrl="https://instagram.com/djsnabu"
    maxPosts={6}
  />
  <ContactSection />
</Layout>
